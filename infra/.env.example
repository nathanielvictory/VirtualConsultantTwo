# /infra/.env

# Db
POSTGRES_HOST=consultant_db
POSTGRES_DB=consultant_db
POSTGRES_USER=consultant_user
POSTGRES_PASSWORD=consultant_pass

# Web API
ConnectionStrings__DefaultConnection='Host=consultant_db;Database=consultant_db;Username=consultant_user;Password=consultant_pass'
ASPNETCORE_ENVIRONMENT=Development

# RabbitMQ
RABBITMQ_HOST=consultant_rabbit
RABBITMQ_PORT=5672
RABBITMQ_VHOST=/
RABBITMQ_USER=consultant
RABBITMQ_PASSWORD=consultant_pass
RABBITMQ_EXCHANGE=app.tasks
RABBITMQ_ROUTINGKEYTASKCREATED=task.created